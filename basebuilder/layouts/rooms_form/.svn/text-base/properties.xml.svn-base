<?xml version="1.0" encoding="ISO-8859-1"?>

<properties>

	<sql>
		<select>*</select>
		<from>rooms</from>
	</sql>

	<key_field>id</key_field>
	<table>rooms</table>
	<navigation>on</navigation>
	<multiple_select_delimiter>::</multiple_select_delimiter>

	<field>
		<field_name>id</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>row</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>column</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>tile_id</field_name>
		<type>integer</type>
		<option_list>6</option_list>
		<option_list_name>tile_list</option_list_name>
	</field>

	<field>
		<field_name>level_id</field_name>
		<type>integer</type>
		<option_list>7</option_list>
		<option_list_name>level_list</option_list_name>
	</field>

	<field>
		<field_name>question_id</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>start_point</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>end_point</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>override_exit_north</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>override_exit_south</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>override_exit_east</field_name>
		<type>integer</type>
	</field>

	<field>
		<field_name>override_exit_west</field_name>
		<type>integer</type>
	</field>


</properties>
